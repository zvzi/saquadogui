Lean.Size = UDim2.new(1, 0, 0, 30)
Lean.Font = Enum.Font.SourceSansBold
Lean.Text = "Lean"
Lean.TextColor3 = Color3.fromRGB(0, 0, 0)
Lean.TextSize = 14.000
Lean.MouseButton1Click:Connect(function()
	stopTracks()
	loadAnimation("rbxassetid://3152375249")
end)

