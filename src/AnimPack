--close gui
local plr = game.Players.LocalPlayer

plr:GetMouse().KeyDown:Connect(function(K)
	if K == "p" then
		Animations.Visible = false
	end
end)
warn("loaded")


-- Stop Animation

plr = game.Players.LocalPlayer
mouse = plr:GetMouse()
mouse.KeyDown:connect(function(key)
	if key == "s" then
		
		stopTracks()
	
	end
end)

